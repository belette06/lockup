
<%= form_for [:home,@appointment] do |f| %>
  <%= f.fields_for :kind do |kind|  %>
     <%= kind.datetime_field :starts_at  %><br/>
     <%= kind.datetime_field :ends_at %><br/>
    <%= f.hidden_field :kind, :value => "appointments" %>
     Recurrence <%=f.check_box :weekly_recurring%><br/>
  Create for tenant
  <%= fields_for :tenant, @tenant do |form| %>
    <%= form.text_field :name %><br/>

    <% end %>
  <%= f.submit %>
<% end %>
<%end %>